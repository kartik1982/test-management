<h1>Listing all Testcases</h1>
<p>
<%= link_to "Create new article", new_testcase_path %>
</p>

<table>
<tr>
<th>Testcase Name</th>
<th>Test Case Description</th>
</tr>
<% @testcases.each do |testcase| %>
<tr>
<td><%= testcase.title %></td>
<td><%= testcase.description %></td>
<td><%= link_to 'Edit', edit_testcase_path(testcase) %></td>
<td><%= link_to 'Show', testcase_path(testcase) %></td>
<td><%= link_to 'Delete', testcase_path(testcase), method: :delete, data: {confirm: "Are you sure?"} %></td>
</tr>
<% end %>
</table>
